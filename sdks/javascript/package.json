{
  "name": "@paradigm/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for Paradigm blockchain network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "paradigm",
    "blockchain",
    "cryptocurrency",
    "web3",
    "sdk",
    "api"
  ],
  "author": "Paradigm Network",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paradigm-network/paradigm-sdk-js"
  },
  "bugs": {
    "url": "https://github.com/paradigm-network/paradigm-sdk-js/issues"
  },
  "homepage": "https://docs.paradigm.network/sdk/javascript",
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "eventemitter3": "^5.0.0",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@types/crypto-js": "^4.2.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}